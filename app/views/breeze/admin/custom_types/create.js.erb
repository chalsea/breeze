<% if @custom_type.errors.empty? %>
  close_tab('new_custom_type');
  $('#left #custom-types').prepend('<%= escape_javascript render(:partial => "custom_type", :object => @custom_type) %>');
  $('#left #custom-types a[data-id=<%= @custom_type.id %>]').click();
<% else %>
  $('#custom_type_new').replaceWith('<%= escape_javascript render(:template => "/breeze/admin/custom_types/new") %>');
<% end %>
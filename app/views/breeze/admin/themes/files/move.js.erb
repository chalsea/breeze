<% @old_id, @new_id = [@old_path, @path].map { |p| "#tab_admin-themes-#{theme.name}-files#{p.gsub(/[\/\.]/, '-')}" }%>
$('a[href=<%= @old_id %>]').attr('href', '<%= @new_id %>').text('<%= escape_javascript File.basename(@path) %>');
$('<%= @old_id %>').attr('id', '<%= @new_id %>').html('<%= escape_javascript render(:file => "/breeze/admin/themes/files/edit.html.erb") %>');